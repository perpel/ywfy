*{
    padding: 0;
    margin: 0;
}

#section-bar {
    width: 100%;
    height: 25px;
    padding: 5px;
    background-color: #E7E5E1;
}
.fnt-ul li {   
    float: left;
    margin-right: 12px;
    padding: 1px 2px;
    list-style: none;
    line-height: 18px;
    text-align: left;
    font-size: 0.8em;
}
.fnt-ul li select{
    
}

.fnt-ul .fnt{
    padding-left: 18px;
    background-repeat: no-repeat;
    background-position: left;
    border: 1px solid #E7E5E1;
    cursor:pointer;
}

.fnt-ul .fnt.active{
    border-color: blue;
    background-color: #C0C0C0;
}

.fnt.ico-refesh{
    background-image:url(../images/ico/retrieve.gif);
    
}

.fnt.ico-import{
    background-image: url(../images/ico/sort.gif);
}

.fnt.ico-add{
    background-image: url(../images/ico/addrow.gif);
}

.fnt.ico-edit{
    background-image: url(../images/ico/new.gif);
}

.fnt.ico-del{
    background-image: url(../images/ico/delrow.gif);
}

.fnt.ico-save-as{
    background-image: url(../images/ico/EXCEL.gif);
}

.fnt.ico-print{
    background-image: url(../images/ico/print.gif);
}

.fnt.ico-search{
    background-image: url(../images/ico/find.gif);
}

.fnt.ico-exit{
    background-image: url(../images/ico/exit.gif);
    float: right;
}
.fnt.ico-save{
    background-image: url(../images/ico/save.gif);
 
}

#myTable tr th{
    white-space: nowrap;
    color: black; 
    background:#d3dcea;
    width:60px;
    line-height:40px; 
    padding:0 10px 0 10px;   
    font-size:14px;
}
#myTable  tr td{ width:100px; font-size:12px; border-bottom:1px solid #cacaca; border-right:1px solid #cacaca; line-height:25px; overflow:hidden; white-space: nowrap;text-overflow:ellipsis; padding:0 5px 0 5px; background:#f8fbff;}
#myTable  tr td input{
    height:26px; float:left; text-indent:1em; width:100%;
}
#import{

    width: 100%;
    background-color: white;
}
#import input{ margin:10px 8px 10px 8px;}
#import input[type=submit]{ background:url(../images/script_btn.png) no-repeat; width:40px; height:20px; border:none; cursor:pointer;}

#import select{ margin:10px 0 10px 0px;}
#import table{

    -moz-user-select: none; /*火狐*/
    -webkit-user-select: none; /*webkit浏览器*/
    -ms-user-select: none; /*IE10*/
    -khtml-user-select: none; /*早期浏览器*/
    user-select: none;
    
}

#import table tr th{
    white-space: nowrap;
}

/*增加弹窗*/
.tablelist{ width:100%; border:1px solid #cacaca; border-bottom:none;}
.tablelist td{ border:1px solid #cacaca; line-height:28px; border-right:none; border-top:none;}

.tablelist td select{ width:100%; border:none; height:22px; float:left; text-indent:1em;}
.tablelist td label{ font-size:14px; float:left;background:#ddecfb; width:100%; height:100%; display:block; text-align:center;}
.tablelist td input{  height:26px; float:left; border:none;text-indent:1em; width:100%;}
.tablelist td textarea{ width:99%; height:50px; resize:none; border:none;}

.tablelist td{position: relative;}
.tablelist td .has-error label{color:red;}
.tablelist td .has-error input{background-color:#E29494 }

.tablelist td .has-error .help-block {position: absolute; top:0px; right:-0px; color:#773B39; font-size:10px;}

.pagination li{float: left; padding: 0px 5px; margin: 0px 3px; background-color: lightblue;}
.pagination .active a{color:red;}